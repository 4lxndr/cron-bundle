{
    "name": "4lxndr/cron-bundle",
    "description": "This bundle provides scheduled execution of Symfony commands",
    "keywords": [
        "cron",
        "cronjob",
        "command",
        "execute",
        "interval",
        "schedule",
        "shapecode",
        "symfony",
        "time",
        "bundle"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://github.com/shapecode/cron-bundle",
    "support": {
        "email": "support@shapeocode.de",
        "issues": "https://github.com/shapecode/cron-bundle/issues",
        "source": "https://github.com/shapecode/cron-bundle/releases",
        "wiki": "https://github.com/shapecode/cron-bundle/wiki"
    },
    "authors": [
        {
            "name": "Nikita Loges",
            "homepage": "https://loges.one",
            "email": "dev@loges.one"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/shapecode/cron-bundle/graphs/contributors"
        }
    ],
    "require": {
        "php": "^8.4",
        "symfony/framework-bundle": "^7.4 || ^8.0",
        "symfony/dependency-injection": "^7.4 || ^8.0",
        "symfony/http-kernel": "^7.4 || ^8.0",
        "symfony/config": "^7.4 || ^8.0",
        "symfony/console": "^7.4 || ^8.0",
        "symfony/process": "^7.4 || ^8.0",
        "symfony/stopwatch": "^7.4 || ^8.0",
        "symfony/event-dispatcher": "^7.4 || ^8.0",
        "symfony/event-dispatcher-contracts": "^3.5 || ^4.0",
        "doctrine/doctrine-bundle": "^2.13 || ^3.0",
        "doctrine/collections": "^2.2",
        "doctrine/persistence": "^4.1",
        "doctrine/orm": "^3.5",
        "doctrine/dbal": "^4.4",
        "psr/clock": "^1.0",
        "dragonmantank/cron-expression": "^3.6"
    },
    "require-dev":{
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpunit/phpunit": "^12.0",
        "symfony/var-dumper": "^7.4 || ^8.0",
        "symfony/clock": "^7.4 || ^8.0",
        "friendsofphp/php-cs-fixer": "^3.91"
    },
    "autoload": {
        "psr-4": {
            "Shapecode\\Bundle\\CronBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shapecode\\Bundle\\CronBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@phpstan",
            "@phpunit"
        ],
        "phpstan": "phpstan analyse --configuration=./phpstan.neon --memory-limit=-1 --ansi",
        "phpstan-update-baseline": "@phpstan --generate-baseline phpstan-baseline.neon --allow-empty-baseline",
        "phpunit": "phpunit --colors=always",
        "cs-check": "vendor/bin/php-cs-fixer fix src --dry-run",
        "cs-fix": "vendor/bin/php-cs-fixer fix src"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "icanhazstring/composer-unused": true
        }
    }
}
